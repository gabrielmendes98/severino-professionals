{
  "name": "severino-professionals",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.js",
  "dependencies": {
    "@date-io/moment": "1.x",
    "@fontsource/roboto": "^4.5.0",
    "@fontsource/ubuntu": "^4.5.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "axios": "^0.21.1",
    "formik": "^2.2.9",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-slick": "^0.28.1",
    "react-toastify": "^7.0.4",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "sh -ac '. ./.env.production; react-scripts start'",
    "start:prod-mock": "export REACT_APP_MOCK_ON=true || set REACT_APP_MOCK_ON=true && sh -ac '. ./.env.production; react-scripts start'",
    "build": "react-scripts build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:w": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.1",
    "msw": "^0.35.0",
    "prettier": "^2.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --coverage"
    }
  },
  "lint-staged": {
    "./src/**/*.+(js|jsx)": [
      "eslint --fix",
      "jest --findRelatedTests"
    ],
    "**/*.+(js|json|md|jsx)": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
